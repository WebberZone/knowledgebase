    "beforeLiItem": {
      "type": "string",
      "default": ""
    },
    "afterLiItem": {
      "type": "string",
      "default": ""
    }
  },
  "example": {},
  "textdomain": "knowledgebase",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css"
